            flex: 1 1 60%;
            display: grid;
            grid-template-columns: repeat(4, 1fr);
            gap: 0.5rem;
            box-sizing: border-box;
          "
        >
          <!-- Large image -->
          <div
            style="
              grid-column: span 2;
              grid-row: span 2;
              overflow: hidden;
              border-radius: 8px;
            "
          >
            <img
              src="seminars/cyber.jpg"
              alt="Nature Scene"
              style="width: 100%; height: 100%; object-fit: cover"
            />
          </div>
          <!-- Smaller images -->
          <div style="overflow: hidden; border-radius: 8px">
            <img
              src="seminars/human1.jpg"
              alt="Explorer"
              style="width: 100%; height: 100%; object-fit: cover"
            />
          </div>
          <div style="overflow: hidden; border-radius: 8px">
            <img
              src="seminars/hostel.jpg"
              alt="Explorer resting"
              style="width: 100%; height: 100%; object-fit: cover"
            />
